<h1>Welcome to Greg's List!</h1>

<h6>(not based off of anything)</h6>

<ul>
  {{#each model.posts as |post|}}
    <li>{{post.title}} {{post.date}}</li>
  {{/each}}
</ul>

<ul>
  {{#each model.categories as |category|}}
    <li>{{#link-to 'category' category.id}} {{category.name}} {{/link-to}}</li>
  {{/each}}

</ul>
